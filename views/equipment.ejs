    <script type="text/javascript">
    	$(document).ready(function() {
			var type = getQueryString('Type');
			if (type == 'transformer') {
				var typeData = [
					{name:"变压器", children:[
						{name:"500kV"},
						{name:'220kV'},
						{name:'110kV'}
					]}
				]
			}
			var config = {
				data:typeData
			}
    		$('.tree').treegrid(config);
      	});
		
		function getQueryString(name) {
     		var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
     		var r = window.location.search.substr(1).match(reg);
     		if (r != null) {
				 return  unescape(r[2]);
			}
			return null;
		}
    </script>
